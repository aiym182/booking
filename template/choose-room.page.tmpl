{{template "base" .}}

{{define "content"}}
    <section class="section">
        <div class="container">
          <h1 class="title">Choose a room</h1>

          {{$rooms := index .Data "rooms"}}

          <ul>
            {{range $rooms}}
              <li>
              <a href="/choose-room/{{.ID}}">{{.RoomName}}</a>
              </li>

            {{end}}
          </ul>
         
        </div>
        
    </section>

{{end}}